{
  "name": "@ampatspell/directus",
  "version": "1.0.6",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "flc": "npm run check && npm run format && npm run lint",
    "patch": "npm version patch --no-git-tag-version"
  },
  "bin": {
    "tools": "./src/lib/tools"
  },
  "files": [
    "dist",
    "src/lib/*",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "svelte": "./dist/utils.js"
    },
    "./base": {
      "types": "./dist/base.d.ts",
      "svelte": "./dist/base.js",
      "import": "./dist/base.js"
    },
    "./directus": {
      "types": "./dist/directus.d.ts",
      "svelte": "./dist/directus.js"
    },
    "./validate": {
      "types": "./dist/validate.d.ts",
      "svelte": "./dist/validate.js"
    }
  },
  "peerDependencies": {
    "@directus/sdk": "^20.1.0",
    "@directus/visual-editing": "^1.1.0",
    "@sveltejs/kit": "^2.43.2",
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.36.0",
    "@sveltejs/adapter-node": "^5.3.2",
    "@sveltejs/kit": "^2.43.2",
    "@sveltejs/package": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@types/node": "^20",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.4",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.13",
    "sass-embedded": "^1.93.2",
    "svelte": "^5.39.5",
    "svelte-check": "^4.3.2",
    "svelte-preprocess": "^6.0.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7"
  },
  "keywords": [
    "svelte"
  ],
  "engines": {
    "node": "24"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "directus-sdk-typegen": "^0.2.1",
    "dotenv": "^17.2.3",
    "p-queue": "^9.0.0"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
