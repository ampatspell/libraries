{
  "name": "@ampatspell/base",
  "version": "1.0.8",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "flc": "npm run check && npm run format && npm run lint",
    "patch": "npm version patch --no-git-tag-version"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./utils/array": {
      "types": "./dist/utils/array.d.ts",
      "svelte": "./dist/utils/array.js"
    },
    "./utils/model": {
      "types": "./dist/utils/model.svelte.d.ts",
      "svelte": "./dist/utils/model.svelte.js"
    },
    "./utils/number": {
      "types": "./dist/utils/number.d.ts",
      "svelte": "./dist/utils/number.js"
    },
    "./utils/object": {
      "types": "./dist/utils/object.d.ts",
      "svelte": "./dist/utils/object.js"
    },
    "./utils/options": {
      "types": "./dist/utils/options.d.ts",
      "svelte": "./dist/utils/options.js"
    },
    "./utils/reactivity": {
      "types": "./dist/utils/reactivity.d.ts",
      "svelte": "./dist/utils/reactivity.js"
    },
    "./utils/scope": {
      "types": "./dist/utils/scope.d.ts",
      "svelte": "./dist/utils/scope.js"
    },
    "./utils/set-global": {
      "types": "./dist/utils/set-global.d.ts",
      "svelte": "./dist/utils/set-global.js"
    },
    "./utils/types": {
      "types": "./dist/utils/types.d.ts",
      "svelte": "./dist/utils/types.js"
    },
    "./utils/promise": {
      "types": "./dist/utils/promise.d.ts",
      "svelte": "./dist/utils/promise.js"
    },
    "./utils/aspect-ratio": {
      "types": "./dist/utils/aspect-ratio.d.ts",
      "svelte": "./dist/utils/aspect-ratio.js"
    },
    "./utils/images": {
      "types": "./dist/utils/images.svelte.d.ts",
      "svelte": "./dist/utils/images.svelte.js"
    },
    "./utils/string": {
      "types": "./dist/utils/string.d.ts",
      "svelte": "./dist/utils/string.js"
    },
    "./components/json": {
      "types": "./dist/components/json.svelte.d.ts",
      "svelte": "./dist/components/json.svelte"
    },
    "./components/background": {
      "types": "./dist/components/background.svelte.d.ts",
      "svelte": "./dist/components/background.svelte"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.36.0",
    "@sveltejs/adapter-node": "^5.3.2",
    "@sveltejs/kit": "^2.43.2",
    "@sveltejs/package": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@types/node": "^20",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.4",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.13",
    "sass-embedded": "^1.93.2",
    "svelte": "^5.39.5",
    "svelte-check": "^4.3.2",
    "svelte-preprocess": "^6.0.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7"
  },
  "keywords": [
    "svelte"
  ],
  "engines": {
    "node": "24"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
